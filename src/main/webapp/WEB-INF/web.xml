<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>ListerCollaborateurController</servlet-name>
    <servlet-class>dev.webapptest.web.ListerCollaborateurController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ListerCollaborateurController</servlet-name>
    <url-pattern>/collaborateurs/lister</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>EditerCollaborateurController</servlet-name>
    <servlet-class>dev.webapptest.web.EditerCollaborateurController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditerCollaborateurController</servlet-name>
    <url-pattern>/collaborateurs/editer</url-pattern>
  </servlet-mapping>
</web-app>
